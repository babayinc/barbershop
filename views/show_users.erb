<H1>Users</H1>
<table class="table">
  <thead>
  	<tr>
  		<th>User</th>
  		<th>Phone</th>
  		<th>Time</th>
  		<th>Barber</th>
  		<th>Color</th>
  	</tr>
  </thead>
  <body>
  	<% @results.each do |row| %>
	  <tr>
	    <td><%= row['user_name'] %></td>
	    <td><%= row['phone'] %></td>
	    <td><%= row['date_time'] %></td>
	    <td><%= row['master'] %></td>
	    <td BGCOLOR="<%= row['colors'] %>" ><%= row['colors'] %></td>
	  </tr>
	<% end %>
  </body>  
</table>